{
  "name": "quantaqueue",
  "version": "0.0.7",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "keywords": [
    "Operations",
    "Research",
    "Operations Research",
    "Queue",
    "Theory",
    "Queue Theory",
    "mathjs",
    "Typescript"
  ],
  "homepage": "https://github.com/gustavoerivero/QuantaQueue#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gustavoerivero/QuantaQueue.git"
  },
  "author": "gustavoerivero",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.7",
    "mathjs": "^13.0.3"
  },
  "scripts": {
    "build": "tsc",
    "release": "npm run build && changeset publish",
    "update:version": "changeset",
    "publish": "npm publish --access=public",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format:check": "prettier . --check",
    "format:write": "prettier . --write",
    "force:check": "eslint src/**/*.ts & prettier . --check",
    "force:fix": "eslint src/**/*.ts --fix & prettier . --write",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint",
      "prettier . --check"
    ],
    "**/*.test.{js,ts}": [
      "jest"
    ]
  }
}
